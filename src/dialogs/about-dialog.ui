<interface>

  <requires lib="gtk+" version="3.24"/>

  <template class="AboutDialog" parent="GtkAboutDialog">
    <property name="modal">True</property>
    <property name="destroy-with-parent">True</property>
    <property name="license-type">gpl-3-0</property>
    <property name="program-name" translatable="yes">Replay</property>
    <property name="copyright">Copyright Â© 2019-2020 Nahuel Gomez Castro</property>
    <property name="authors">Nahuel Gomez Castro &lt;nahual_gomca@outlook.com.ar&gt;</property>
    <!-- Translators: This is the comment for the About dialog -->
    <property name="comments" translatable="yes">An open source YouTube client for GNOME.</property>
    <!-- Translators: This is the label used to link to the project repo -->
    <property name="website-label" translatable="yes">Project repository</property>
    <signal name="response" handler="on_response"/>
  </template>
</interface>
